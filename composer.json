{
    "name": "star/event-storming",
    "description": "Tool for building event",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ramsey/uuid": "^3.8",
        "star/domain-event": "^2.0",
        "symfony/console": "^4.1"
    },
	"require-dev": {
        "infection/infection": "^0.10.5",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-phpunit": "*",
        "phpstan/phpstan-symfony": "^0.11.6",
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
           "Star\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "Star\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true
    }
}
